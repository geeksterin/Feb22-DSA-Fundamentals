 function sortColors() 
 {
		var inputArray = [0,2,2,1,1,0,0,2,1,2];
		var start = 0;        
		var end = inputArray.length - 1; // 9
		for(let i = 0;i<=end;i++){
			if(inputArray[i] == 0)
			{
				let tempVar = inputArray[i];
				inputArray[i] = inputArray[start]; // 
				inputArray[start] = tempVar;
				start++;
				
			}else if(inputArray[i] == 2){
				let tempVar = inputArray[i];
				inputArray[i] = inputArray[end];
				inputArray[end] = tempVar;
				end--;
				i--;
			}
		}
	
		console.log(inputArray);
		
	}
	
sortColors();
	
